<?xml version="1.0"?>
<config>
    <modules>
        <JR_Api>
            <version>1.0.0</version>
        </JR_Api>
    </modules>
    <global>
        <helpers>
            <jr_api>
                <class>JR_Api_Helper</class>
            </jr_api>
        </helpers>
        <models>
            <catalog>
                <rewrite>
                    <product_api>JR_Api_Model_Catalog_Product_Api</product_api>
                    <product_api_v2>JR_Api_Model_Catalog_Product_Api_V2</product_api_v2>
                </rewrite>
            </catalog>
        </models>
    </global>
    <default>
        <api>
            <config>
                <categories_separator><![CDATA[//]]></categories_separator>
            </config>
        </api>
    </default>
</config>